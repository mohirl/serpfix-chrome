{
  "manifest_version": 2,
  
  "name": "Fix SERPs",
  "description": "Revert painful new SERP format closer to previous version",
  "version": "0.1",

	
	"web_accessible_resources": [ "fix.css"	],
  
  "page_action": {
    "default_title": "Fix SERPs"
    },
	"content_scripts": [
	{
		"matches": ["https://www.google.ie/*", "https://www.google.coxm/*", "https://www.google.co.uk/*"],
		"css" : ["fix.css"],
		"run_at": "document_idle"
	}
	]


}
